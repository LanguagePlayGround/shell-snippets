{
    "bash":{
        "prefix": "bash",
        "body": "#!/bin/bash\n",
        "description": "executer"
    },
    "echo":{
        "prefix": "echo",
        "body": "echo ${0:massage}",
        "description": "echo message"
    },
    "if" : {
        "prefix": "if",
        "body": "if ${0:condition} ;then\n\t${1}\nfi",
        "description": "if"
    },
    "elseif" : {
        "prefix": "elseif",
        "body": "elif ${0:condition} ;then\n\t${1}",
        "description": "elseif"
    },
    "else" : {
        "prefix": "else",
        "body": "else\n\t${0}",
        "description": "else"
    },
    "file_exist": {
        "prefix": "file_exist",
        "body": " -f ${0:$FILE_NAME} ",
        "description": "-f $FILENAME #test file exist"
    },
    "test_file_exist": {
        "prefix": "test_file_exist",
        "body": "[ -f ${0:$FILE_NAME} ]",
        "description": "[ -f $FILE_NAME ] #test file exist"
    },
    "if_file_exist" : {
        "prefix": "if_file_exist",
        "body": "if [ -f ${0:$FILE_NAME} ];then\n\t${1}\nfi",
        "description": "if file exist"
    },
    "forin" : {
        "prefix": "forin",
        "body": "for ${0:VAR} in $${1:LIST}\ndo\n\techo $${0:VAR}\ndone\n",
        "description": "for loop in list"
    }
}